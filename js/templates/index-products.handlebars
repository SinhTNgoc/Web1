{{#each data}}
  <div class="col text-center">
    <img src="{{thumbpath}}" alt="Coffee" />
    <h4>{{name}}</h4>
    <div class="pb-2">{{summary}}</div>
    <a
      class="read-more"
      href="#"
      onclick="loadData('products/{{id}}', '#content', 'index-details')"
    >Read more</a>
  </div>
{{/each}}